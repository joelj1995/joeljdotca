{
  "name": "blog-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "pm2 start ecosystem.config.js --no-daemon",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run blog-app:serve-ssr",
    "serve:ssr": "node dist/blog-app/server/main.js",
    "build:ssr": "git rev-parse --short HEAD > ./src/assets/version.txt && ng build && ng run blog-app:server && echo { } > ./dist/applicationinsights.json",
    "prerender": "ng run blog-app:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.9",
    "@angular/common": "^16.2.9",
    "@angular/compiler": "^16.2.9",
    "@angular/core": "^16.2.9",
    "@angular/forms": "^16.2.9",
    "@angular/localize": "^16.2.9",
    "@angular/platform-browser": "^16.2.9",
    "@angular/platform-browser-dynamic": "^16.2.9",
    "@angular/platform-server": "^16.2.9",
    "@angular/router": "^16.2.9",
    "@brokerloop/ttlcache": "^3.2.3",
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@microsoft/applicationinsights-web": "^2.8.6",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@nguniversal/express-engine": "^16.2.0",
    "@popperjs/core": "^2.10.2",
    "applicationinsights": "^2.8.0",
    "bootstrap": "^5.1.3",
    "contentful": "^9.0.1",
